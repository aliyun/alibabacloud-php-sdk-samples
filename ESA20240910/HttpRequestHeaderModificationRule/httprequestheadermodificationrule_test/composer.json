{
  "name": "alibabacloud/codesample",
  "type": "library",
  "description": "Alibaba Cloud Code Sample for PHP",
  "github": "https://github.com/aliyun/alibabacloud-php-sdk-samples",
  "main": "src/Client.php",
  "authors": [
    {
      "name": "Alibaba Cloud SDK",
      "email": "sdk-team@alibabacloud.com"
    }
  ],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "AlibabaCloud\\CodeSample\\": "src"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "require": {
    "php": ">5.5",
    "alibabacloud/darabonba": "^1.0.0",
    "alibabacloud/credentials": "^1.2.2",
    "alibabacloud/esa-20240910": "^2.31.1",
    "alibabacloud/tea-utils": "^0.2.21",
    "alibabacloud/darabonba-openapi": "^0.2.16"
  },
  "prefer-stable": true
}
